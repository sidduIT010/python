# Complete the solve function below.
def solve(s):
    result = ""
    capitalize = True

    for ch in s:
        if capitalize and ch.isalpha():
            result += ch.upper()
            capitalize = False
        else:
            result += ch
            capitalize = (ch == " ")

    return result




